{
  "version": 2,
  "builds": [
    {
      "src": "restaurant-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "restaurant-frontend/$1"
    }
  ],
  "installCommand": "cd restaurant-frontend && npm install",
  "buildCommand": "cd restaurant-frontend && npm run build",
  "outputDirectory": "restaurant-frontend/build"
}